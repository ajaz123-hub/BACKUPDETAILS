package java8;
import java.util.*;
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
String s="amerkhan";


TreeMap<Character,Integer> freq=new TreeMap<Character,Integer>();

for(int i=0;i<s.length();i++)
{
	char c=s.charAt(i);
	
	if(freq.containsKey(c))
	{
		freq.put(c, freq.get(c)+1);
	}
	else
	{
		freq.put(c,1);
	}
}



	}

}
