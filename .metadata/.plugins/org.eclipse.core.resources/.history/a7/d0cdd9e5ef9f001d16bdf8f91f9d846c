package ArrayPrograms;

import java.util.Arrays;

public class EvenOddArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// TODO Auto-generated method stub
int a[]= {1,3,4,6,5,9,7};
int count=0;

for(int i=0;i<a.length;i++)
{
	if(a[i]%2==0)
	{
		System.out.println(a[i]);
	}
}
for(int i=0;i<a.length;i++)
{
	if(a[i]%2!=0)
	{
		System.out.println(a[i]);
	}
}



	}

}
