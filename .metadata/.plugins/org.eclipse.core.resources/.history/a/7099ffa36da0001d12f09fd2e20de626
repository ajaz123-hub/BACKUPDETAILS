package ArrayPrograms;

import java.util.Arrays;

public class RototaArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]= {7,2,6,8,1};
		int count=0;
		int temp=a[0];//7
		//2,6,8,1
		
		for(int i=0;i<a.length-1;i++)
		{
		a[i]=a[i+1];
		count++;
		}
		a[count]=temp;
		System.out.println(Arrays.toString(a));
	}

}
