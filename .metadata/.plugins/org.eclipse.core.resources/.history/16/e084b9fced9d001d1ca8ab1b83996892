package java8;

public class Check {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
String s="   Mohd    Amer  khan    ";

System.out.println(s.trim());
int start=0;
int end=0;

for(int i=0;i<s.length();i++)
{
	if(s.charAt(i)!=' ')
	{
		start=i;
		break;
	}
}
for(int i=s.length()-1;i<s.length();i--)
{
	if(s.charAt(i)!=' ')
	{
		end=i;
		break;
	}
}

System.out.println(start +" "+end);
	}

}
